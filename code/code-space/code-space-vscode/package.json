{
  "name": "code-space-vscode",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "activationEvents": [
    "onCommand:codeSpace.start"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "codeSpace.start",
        "title": "Start new code space session",
        "category": "Code Space"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ../../node_modules/vscode/bin/install"
  },
  "dependencies": {
    "react": "^16.12.0",
    "vscode": "*"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "typescript": "^2.8.3"
  }
}